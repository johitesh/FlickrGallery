    pod 'Alamofire'
    pod 'AlamofireImage'
    pod "HanekeSwift", :git => 'https://github.com/Haneke/HanekeSwift.git', :branch => 'feature/swift-3'
end

  target 'FlickrGalleryTests' do
    pod ‘Quick’
    pod ‘Nimble’
  end

  target 'FlickrGalleryUITests' do
    inherit! :search_paths
    # Pods for testing
  end